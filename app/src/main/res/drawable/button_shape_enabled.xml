<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:shape="rectangle"
    tools:ignore="MissingDefaultResource">
    <corners
        android:radius="30dp"
        />
    <gradient
        android:angle="180"
        android:centerX="50%"
        android:centerColor="#051EFF"
        android:startColor="#2ED914"
        android:endColor="#F7FF00"
        android:type="linear"
        />
    <padding
        android:left="30dp"
        android:top="5dp"
        android:right="30dp"
        android:bottom="5dp"
        />
    <stroke
        android:width="3dp"
        android:color="#000000"
        />
</shape>